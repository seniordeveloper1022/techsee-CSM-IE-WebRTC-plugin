<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="14.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|Win32'">
    <WebBrowserDebuggerHttpUrl>$(SolutionDir)..\CSM-IE-WebRTC-shim\test\dc.html</WebBrowserDebuggerHttpUrl>
    <DebuggerFlavor>WebBrowserDebugger</DebuggerFlavor>
    <LocalDebuggerCommand>regsvr32.exe</LocalDebuggerCommand>
    <LocalDebuggerCommandArguments>$(TargetPath)</LocalDebuggerCommandArguments>
    <LocalDebuggerAttach>false</LocalDebuggerAttach>
    <LocalDebuggerDebuggerType>NativeOnly</LocalDebuggerDebuggerType>
    <LocalDebuggerAmpDefaultAccelerator>
    </LocalDebuggerAmpDefaultAccelerator>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|Win32'">
    <WebBrowserDebuggerHttpUrl>$(ProjectDir)WebRTCProxy.htm</WebBrowserDebuggerHttpUrl>
    <DebuggerFlavor>WebBrowserDebugger</DebuggerFlavor>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <WebBrowserDebuggerHttpUrl>$(ProjectDir)WebRTCProxy.htm</WebBrowserDebuggerHttpUrl>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
    <LocalDebuggerCommand>C:\Windows\system32\regsvr32.exe</LocalDebuggerCommand>
    <LocalDebuggerCommandArguments>/i $(TargetPath)</LocalDebuggerCommandArguments>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
    <WebBrowserDebuggerHttpUrl>$(ProjectDir)WebRTCProxy.htm</WebBrowserDebuggerHttpUrl>
    <DebuggerFlavor>WebBrowserDebugger</DebuggerFlavor>
  </PropertyGroup>
  <PropertyGroup>
    <ShowAllFiles>false</ShowAllFiles>
  </PropertyGroup>
</Project>